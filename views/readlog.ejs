<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <title>RingCentral Smart Transcription</title>
    <link href="./img/favicon.png" type="image/vnd.microsoft.icon" rel="shortcut icon">
    <link href="./img/favicon.png" type="image/vnd.microsoft.icon" rel="icon">
    <script src="js/main.js" type="text/javascript"></script>
    <script src="js/jquery-3.1.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/css/bootstrap-select.min.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="css/ui.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/js/bootstrap-select.min.js"></script>
</head>
<body onload="initForReadLog()">
  <nav class="navbar navbar-default no-border">
    <span class="left-middle-align">
      <span class="rclogo"><a href="/"><img alt="Brand" src="img/ringcentral-labs.png" height="40"></a></span>&nbsp;&nbsp;
      <span class="title rc-orange">Smart Transcription</span>
    </span>
    <ul class="nav navbar-nav navbar-left left-align-nav">
      <li class="active"><a href="/readlog">Imports</a></li>
      <li><a href="/recordedcalls">Data List</a></li>
      <li><a href="/enrollspeaker">Speaker Enrollment</a></li>
    </ul>
    <ul class="nav navbar-nav navbar-right middle-align">
      <li><span><b><%- userName %>&nbsp;&nbsp;</b></span></li>
      <li><a href="/about">&nbsp;&nbsp;About</a></li>
      <li><span>&nbsp;&nbsp;|&nbsp;&nbsp;</span></li>
      <li><a href="/logout">Logout</a></li>
    </ul>
  </nav>
  <section id='content'>
    <div>
    <h3>Read call recordings or meeting recordings</h3><br/>
    <form class="form-inline">
      <div class="form-group">
        <label for="fromDate">From Date</label>
          <input type="text" id="fromdatepicker" class="form-control text-input" size="10"></input>&nbsp;&nbsp;
        <label for="toDate">To Date</label>
          <input type="text" id="todatepicker" class="form-control text-input" size="10"></input>&nbsp;

        <% if (autoProcessingOn == true) { %>
          <button type="button" class="btn-rc" onclick="enableNotification(event)" id="notification_btn" style="display:inline">Disable Auto Processing</button>
        <% } else { %>
          <button type="button" class="btn-rc" onclick="enableNotification(event)" id="notification_btn" style="display:inline">Enable Auto Processing</button>
        <% } %>
        <span>(Voice call only)</span>
        <br/><br/>
        <button id="readcalllogs" class="btn-rc" onclick="readCallLogs(event)" style="display:inline">Read Call Recordings</button>
        <button id="readmeeting" class="btn-rc" onclick="readMeetings(event)" style="display:inline">Read Meeting Recordings</button>
      </div>
      <img src='./img/loading.gif' id="logginIcon" style='width:30px;height:30px;display: none'></img>
    </form>
    </div>
  </section>
  <%- include('_footer') %>
</body>
</html>
